<%@tag import="java.util.Date"%>
<%@ tag language="java" pageEncoding="UTF-8"%>

<!-- 定义当前标签体的属性 -->
<%@ attribute name="count" required="true" type="java.lang.Integer" %>

Time: <%= new Date() %>

<br><br>
<!-- 访问标签体的内容 -->
<jsp:doBody></jsp:doBody>

<br><br>
<!-- 直接访问属性值. 就是当前 JSP 页面的一个成员变量 -->
count: <%= count %>